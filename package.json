{
    "name": "vscode-archive",
    "displayName": "vscode-archive",
    "description": "compress/decompress .zip/.vsix/.crx",
    "version": "0.0.1",
    "publisher": "YuTengjing",
    "engines": {
        "vscode": "^1.63.1"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "compress",
        "decompress",
        "zip",
        "unzip",
        "crx",
        "vsix"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tjx666/vscode-archive"
    },
    "activationEvents": [
        "onCommand:vscode-archive.decompress"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-archive.decompress",
                "title": "Decompress Here"
            }
        ],
        "menus": {
          "explorer/context": [
            {
              "command": "vscode-archive.decompress",
              "when": "!explorerResourceIsFolder && resourceFilename =~ /.+\\.(zip|vsix|crx)$/i",
              "group": "navigation@0"
            }
          ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "dependencies": {
        "jszip": "3.7.1",
        "unzipper": "0.10.11"
    },
    "devDependencies": {
        "@types/glob": "7.2.0",
        "@types/mocha": "9.1.0",
        "@types/node": "17.0.10",
        "@types/unzipper": "0.10.5",
        "@types/vscode": "1.63.1",
        "@typescript-eslint/eslint-plugin": "5.10.0",
        "@typescript-eslint/parser": "5.10.0",
        "@vscode/test-electron": "2.1.1",
        "eslint": "8.7.0",
        "eslint-config-prettier": "8.3.0",
        "glob": "7.2.0",
        "mocha": "9.1.4",
        "prettier": "2.5.1",
        "typescript": "4.5.5"
    }
}
