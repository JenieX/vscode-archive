{
    "name": "vscode-archive",
    "displayName": "vscode-archive",
    "description": "compress/decompress .zip/.vsix/.crx/.asar",
    "version": "0.1.2",
    "publisher": "YuTengjing",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "compress",
        "decompress",
        "zip",
        "unzip",
        "crx",
        "vsix",
        "asar"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tjx666/vscode-archive"
    },
    "activationEvents": [
        "onCommand:vscode-archive.decompress"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-archive.decompress",
                "title": "Decompress Here"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "vscode-archive.decompress",
                    "when": "!explorerResourceIsFolder && resourceFilename =~ /.+\\.(zip|vsix|crx|asar)$/i",
                    "group": "navigation@0"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "pnpm esbuild:base --minify",
        "preinstall": "npx only-allow pnpm",
        "clear": "rimraf ./out",
        "esbuild:base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --external:original-fs --format=cjs --platform=node",
        "esbuild:watch": "pnpm esbuild:base --sourcemap --watch",
        "compile:test": "pnpm clear && tsc -p ./test/tsconfig.json",
        "package": "vsce package --no-dependencies",
        "publish": "vsce publish --no-dependencies"
    },
    "dependencies": {
        "asar": "3.2.0",
        "jszip": "3.10.1"
    },
    "devDependencies": {
        "@types/glob": "8.0.1",
        "@types/mocha": "10.0.1",
        "@types/node": "16.18.11",
        "@types/unzipper": "0.10.5",
        "@types/vscode": "1.74.0",
        "@typescript-eslint/eslint-plugin": "5.48.2",
        "@typescript-eslint/parser": "5.48.2",
        "@vscode/test-electron": "2.2.2",
        "esbuild": "0.17.0",
        "eslint": "8.32.0",
        "eslint-config-prettier": "8.6.0",
        "glob": "8.1.0",
        "mocha": "10.2.0",
        "prettier": "2.8.3",
        "typescript": "4.9.4"
    }
}
