{
    "name": "vscode-archive",
    "displayName": "vscode-archive",
    "description": "compress/decompress .zip/.vsix/.crx/.asar",
    "version": "0.1.1",
    "publisher": "YuTengjing",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "compress",
        "decompress",
        "zip",
        "unzip",
        "crx",
        "vsix",
        "asar"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tjx666/vscode-archive"
    },
    "activationEvents": [
        "onCommand:vscode-archive.decompress"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-archive.decompress",
                "title": "Decompress Here"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "vscode-archive.decompress",
                    "when": "!explorerResourceIsFolder && resourceFilename =~ /.+\\.(zip|vsix|crx|asar)$/i",
                    "group": "navigation@0"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "rm -rf ./out && npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "dependencies": {
        "asar": "3.1.0",
        "jszip": "3.7.1"
    },
    "devDependencies": {
        "@types/glob": "7.2.0",
        "@types/mocha": "9.1.0",
        "@types/node": "17.0.18",
        "@types/unzipper": "0.10.5",
        "@types/vscode": "1.64.0",
        "@typescript-eslint/eslint-plugin": "5.12.0",
        "@typescript-eslint/parser": "5.12.0",
        "@vscode/test-electron": "2.1.2",
        "eslint": "8.9.0",
        "eslint-config-prettier": "8.4.0",
        "glob": "7.2.0",
        "mocha": "9.2.1",
        "prettier": "2.5.1",
        "typescript": "4.5.5"
    }
}
